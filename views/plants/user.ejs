<!--views/plants/user.ejs-->

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
</head>
<body>
  <h1>Explore Plant Lovers</h1>
<ul>
  <% users.forEach(user => { %>
    <li>
      <a href="/users/<%= user._id %>/plants"><%= user.username %>'s Plants</a>
    </li>
  <% }) %>
</ul>
  
</body>
</html>